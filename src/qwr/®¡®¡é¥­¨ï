interface Playable <T>{
    public void addSong(T t);
    public T playSong();
}
class Player implements Playable<Music>{
    private Music song;
    @Override
    public void addSong(Music song){
        this.song = song;
    }
    @Override
    public Music playSong(){
        return song;
    }
}

