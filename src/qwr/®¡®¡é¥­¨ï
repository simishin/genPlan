class Player <T, U> {
    private T song;
    private U genre;
    public void addSong(T song, U genre){
        this.song = song;
        this.genre = genre;
    }
    public T playSong(){
        return song;
    }
}

public class Example{
    public static void main(String... args){
        Music music = new Music();
        Player<Music, String> mp3Player = new Player<>();
        mp3Player.addSong(music, "Rock");
    }
}

